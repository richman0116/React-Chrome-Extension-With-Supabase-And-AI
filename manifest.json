{
  "version": "1.0.9",
  "manifest_version": 3,
  "name": "Eden - Build Circles Anywhere",
  "description": "Chrome extension for Eden",
  "action": {
    "default_popup": "js/index.html",
    "default_title": "React Chrome Extension"
  },
  "icons": {
    "128": "icon-128.png",
    "34": "icon-34.png"
  },
  "background": {
    "service_worker": "js/background.bundle.js"
  },
  "content_scripts": [
    {
      "matches": ["*://*/*"],
      "js": ["js/content.bundle.js"]
    }
  ],
  "permissions": ["tabs", "scripting", "activeTab", "storage", "history", "identity"],
  "host_permissions": ["http://*/*", "https://*/*"],
  "oauth2": {
    "client_id": "135731472785-s9i8q6hu7havhdskgjekscfmp6pr96rf.apps.googleusercontent.com",
    "scopes": ["openid", "email", "profile"]
  }
}
